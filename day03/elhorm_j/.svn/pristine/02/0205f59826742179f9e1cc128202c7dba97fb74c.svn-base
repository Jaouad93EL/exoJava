public class Tiger extends Mammal {

    public Tiger(String name, float weight) {
	super(name, weight);
    }

    @Override
    public void say() {
	System.out.println(this.name + " (weight : "+ this.weight +") say : I will eat you! (" + this.weight +")");
			   
    }
    
    @Override
    public void run() {
	System.out.println(this.name + "I'm running!");
    }
}
