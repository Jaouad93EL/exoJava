public class Elephant extends Mammal {

    public Elephant(String name, float weight) {
	super(name, weight);
    }

    @Override
    public void say() {
        System.out.println(this.name + " say : I'm BIG and HEAVY! (weight : " + this.weight + ")");
    }

    @Override
    public void run() {
	System.out.println(this.name + "I'm running!");
    }
}
