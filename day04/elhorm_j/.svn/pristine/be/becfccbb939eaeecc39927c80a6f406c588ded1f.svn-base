public class Desk {

    private Shape[] shapes;

    public Desk() {
	this.shapes = new Shape[1000];
    }

    public void draw() {
	for (int  i = 0; i < this.shapes.length; ++i)
	    if (this.shapes[i] != null)
		this.shapes[i].draw();
    }

    public void addShape(Shape shape) {
	for (int i = 0; i < this.shapes.length; ++i)
	    if (this.shapes[i] == null) {
		this.shapes[i] = shape;
		return;
	    }
    }
    public void addShapes(Shape[] shapes) {
	int add = -1;
	for (int i = 0; i < this.shapes.length; ++i)
	    if (this.shapes[i] == null)
		this.shapes[i] = shapes[++add];
    }    
}
